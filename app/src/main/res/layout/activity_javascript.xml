<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <WebView
        android:id="@+id/webView"
        android:layout_width="match_parent"
        android:layout_height="300sp" >
    </WebView>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="90sp"
        android:id="@+id/azureTV"
        android:text="{ \n Results \n{ asd}"
        />

    <Button
        android:id="@+id/testAzureBTN"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Test Azure ML Studio" />

    <Button
        android:id="@+id/testDatadotGovBTN"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Test Data.gov" />

    <Button
        android:id="@+id/test_sp_today_BTN"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Test sp-today" />
</LinearLayout>