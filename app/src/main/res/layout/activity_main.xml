<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="mounla.hani.datavisualizer.MainActivity">

    <view
        class="com.github.mikephil.charting.charts.BarChart"
        android:id="@+id/barChart"
        layout_alignParentTop="true"
        layout_centerHorizontal="true"
        layout_marginTop="174dp"
        android:layout_width="match_parent"
        android:layout_height="300sp"
        android:layout_marginTop="23dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true"
         />

    <view
        class="com.rey.material.widget.Button"
        android:id="@+id/powerBIBTN"
        layout_centerHorizontal="true"
        layout_marginTop="96dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/barChart"
        android:text="Power BI Report View"
        android:layout_alignParentStart="true"
        android:layout_marginTop="52dp" />

    <view
        class="com.rey.material.widget.Button"
        android:id="@+id/contactsJsonBTN"
        layout_centerHorizontal="true"
        layout_marginTop="96dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/powerBIBTN"
        android:text="Contacts Json Sample"
        android:layout_alignParentStart="true"/>

    <view
        class="com.rey.material.widget.Button"
        android:id="@+id/javaScriptBTN"
        layout_centerHorizontal="true"
        layout_marginTop="96dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/contactsJsonBTN"
        android:text="Java Script Activity"
        android:layout_alignParentStart="true"/>
</RelativeLayout>
